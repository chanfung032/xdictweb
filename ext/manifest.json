{
   "background_page": "background.html",
   "browser_action": {
      "default_icon": "static/icon19.png",
      "default_popup": "popup.html",
      "default_title": "Google Dictionary"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content.css" ],
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls\u003E" ]
   } ],
   "description": "View definitions easily as you browse the web.",
   "icons": {
      "128": "static/icon128.png",
      "32": "static/icon32.png",
      "48": "static/icon48.png"
   },
   "name": "Google Dictionary (by Google)",
   "options_page": "options.html",
   "permissions": [ "tabs", "http://clients5.google.com/*", "http://xdictweb.sinaapp.com/*" ],
   "version": "3.0.6"
}
